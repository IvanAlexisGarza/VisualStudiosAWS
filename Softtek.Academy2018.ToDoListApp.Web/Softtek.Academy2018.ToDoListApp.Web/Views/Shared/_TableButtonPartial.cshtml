@model Softtek.Academy2018.ToDoListApp.Web.Models.IndividualButtonPartial

@using Softtek.Academy2018.ToDoListApp.Web.Models

<td style="width:400px">
    <div class="btn-group" role="group">
        @Html.Partial("_IndividualButtonPartial", new IndividualButtonPartial
   {
       Action = "Edit",
       ButtonType = "btn-primary",
       GlyphIcon = "pencil",
       CosaId = Model.CosaId

   })
        @Html.Partial("_IndividualButtonPartial", new IndividualButtonPartial
   {
       Action = "Details",
       ButtonType = "btn-success",
       GlyphIcon = "list",
       CosaId = Model.CosaId
   })
        @Html.Partial("_IndividualButtonPartial", new IndividualButtonPartial
   {
       Action = "Delete",
       ButtonType = "btn-danger",
       GlyphIcon = "trash",
       CosaId = Model.CosaId
   })
    </div>
</td>

